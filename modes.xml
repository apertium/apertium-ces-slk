        <file name="cs-sk.autobil.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-cs-sk.cs-sk.t2x"/>
        <file name="cs-sk.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-cs-sk.cs-sk.t3x"/>
        <file name="cs-sk.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="cs-sk.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="cs-sk.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

